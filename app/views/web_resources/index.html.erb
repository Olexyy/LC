<p id="notice"><%= notice %></p>

<h1>Web Resources</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Body</th>
      <th>Key</th>
      <th>Weight</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @web_resources.each do |web_resource| %>
      <tr>
        <td><%= web_resource.name %></td>
        <td><%= web_resource.title %></td>
        <td><%= web_resource.body %></td>
        <td><%= web_resource.key %></td>
        <td><%= web_resource.weight %></td>
        <td><%= link_to 'Show', web_resource %></td>
        <td><%= link_to 'Edit', edit_web_resource_path(web_resource) %></td>
        <td><%= link_to 'Destroy', web_resource, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Web Resource', new_web_resource_path %>
